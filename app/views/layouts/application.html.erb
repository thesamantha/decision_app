<!DOCTYPE html>
<html>
<head>
  <title><%= full_title(yield(:title)) %></title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <%= render 'layouts/shim' %> <!-- insert contents of _shim.html.erb -->
</head>
<body>
  <%= render 'layouts/header' %>  <!-- insert contents of _header.html.erb -->
  <div class="container">
    <%= yield %>
  </div>
  <%= render 'layouts/footer' %> <!-- insert contents of _footer.html.erb -->
</body>
</html>
